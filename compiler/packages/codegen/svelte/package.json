{
  "name": "@uih-dsl/codegen-svelte",
  "version": "0.0.3",
  "description": "Svelte code generator for UIH DSL - Generate Svelte 4/5 components",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -b",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "uih",
    "dsl",
    "codegen",
    "svelte",
    "svelte5",
    "compiler",
    "ui",
    "ai"
  ],
  "author": "UIH DSL Team",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@uih-dsl/ir": "workspace:*"
  },
  "devDependencies": {
    "@uih-dsl/parser": "workspace:*",
    "@uih-dsl/tokenizer": "workspace:*",
    "typescript": "^5.7.3",
    "vitest": "^3.0.0"
  }
}
