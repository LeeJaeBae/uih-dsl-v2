    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "uih",
    "dsl",
    "runtime",
    "react"
  ],
  "author": "UIH DSL Team",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@uih-dsl/runtime-core": "workspace:*"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "jsdom": "^27.2.0",
    "typescript": "^5.7.3",
    "vitest": "^3.0.0"
  }
}
