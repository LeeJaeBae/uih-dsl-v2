{
  "name": "@uih-dsl/runtime-react",
  "version": "1.0.1",
  "description": "UIH DSL Runtime for React",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
