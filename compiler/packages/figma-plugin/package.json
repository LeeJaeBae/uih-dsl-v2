{
  "name": "@uih-dsl/figma-plugin",
  "version": "0.1.0",
  "description": "Figma plugin to convert designs to UIH DSL",
  "main": "dist/code.js",
  "scripts": {
    "build": "rm -rf dist && vite build -c vite.config.ui.ts && vite build -c vite.config.code.ts",
    "dev": "npm run watch",
    "watch": "npm run build -- --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@uih-dsl/codegen-react": "^0.0.2",
    "@uih-dsl/ir": "^0.0.2",
    "@uih-dsl/parser": "^0.0.2",
    "@uih-dsl/tokenizer": "^0.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@figma/plugin-typings": "^1.84.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.0",
    "typescript": "^5.2.0",
    "vite": "^5.0.0",
    "vite-plugin-singlefile": "^2.0.1"
  }
}
