{
  "name": "@uih-dsl/cli",
  "version": "1.0.1",
  "description": "UIH DSL Command Line Interface",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "uih": "dist/index.js"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsx src/index.ts",
    "test": "vitest run"
  },
  "keywords": [
    "uih",
    "dsl",
    "cli",
    "compiler"
  ],
  "author": "UIH DSL Team",
  "license": "MIT",
  "publishConfig": {
