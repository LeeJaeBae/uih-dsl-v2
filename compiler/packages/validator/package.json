{
  "name": "@uih-dsl/validator",
  "version": "0.0.2",
  "description": "Validator for UIH DSL AST and IR",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
