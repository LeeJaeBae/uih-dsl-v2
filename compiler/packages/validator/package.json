    "dsl",
    "validator",
    "compiler"
  ],
  "author": "UIH DSL Team",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@uih-dsl/tokenizer": "workspace:*",
    "@uih-dsl/parser": "workspace:*",
    "@uih-dsl/ir": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.0.0"
  }
